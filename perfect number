def perfect(n):
    
    sum=0
    temp = n
    for i in range(1,n+1):
        
        if(n%i==0 and n!=i):
           sum = sum + i
           print(i)
           
    if (sum == temp):
        print("Its a perfect number")
        
    else:
        print("its not a perfect number")   
     
    
n = int(input("Enter a number: "))
perfect(n)
